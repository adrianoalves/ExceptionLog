{
    "name": "adrianoalves/laravel-exceptionlog",
    "description": "Simple Laravel Exception and Error Persistence Log Layer",
    "keywords": [ "laravel", "logging", "log", "exception", "mysql", "php", "php 7" ],
    "type": "laravel-package",
    "license": "MIT",
    "require": {
        "php": ">7.2",
        "illuminate/support": ">=6"
    },
    "require-dev": {
    },
    "authors": [
        {
            "name": "Adriano Alves",
            "email": "adriano@venddi.com.br"
        }
    ],
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "ExceptionLog\\" : "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "ExceptionLog\\ExceptionLogProvider"
            ],
            "aliases": {
                "ExceptionLog": "ExceptionLog\\ExceptionLog"
            }
        }
    }
}
